@using System.Security.Policy

@{
    ViewBag.Title = "AnAuthorised";
    Layout = "~/Views/Shared/_ErrorLayout.cshtml";
}

<section class="content">
    <div class="error-page">
        <h2 class="headline text-yellow"> 401</h2>

        <div class="error-content">
            <h3><i class="fa fa-warning text-yellow"></i> Oops! Page not authorized.</h3>

            <p>
                You are not authorized to view the page you're navigating to.

                Meanwhile, you may <a href="">return to previous page</a> or contact the administrator.
            </p>

        </div>
        <!-- /.error-content -->
    </div>
    <!-- /.error-page -->
</section>


