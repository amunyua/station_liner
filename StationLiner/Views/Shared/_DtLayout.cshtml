<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title | FCS</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @* @Scripts.Render("~/bundles/modernizr")*@
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="@Url.Content("~/Content/plugins/datatables/dataTables.bootstrap.css")">
    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="@Url.Content("~/Content/plugins/iCheck/all.css")">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Content/dist/css/AdminLTE.min.css")">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/Content/dist/css/skins/_all-skins.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/plugins/select2/select2.min.css")">

   

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @{Html.RenderPartial("partials/_Header"); }

        <!-- Left side column. contains the logo and sidebar -->
        @{Html.RenderPartial("partials/_SideMenu"); }

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @RenderSection("PageTitle", false)
                    @*<small>advanced tables</small>*@
                </h1>
                <ol class="breadcrumb">
                    @RenderSection("BreadCrumbs", false)
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <ul class="list-unstyled pull-right">
                            @RenderSection("Actions", false)
                        </ul>
                        <br /><br />
                        <div class="box box-solid box-primary">
                            <div class="box-header">
                                <h3 class="box-title">@RenderSection("TableHeader", false)</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">
                                @RenderBody()
                            </div>
                            <!-- /.box-body -->
                        </div>

                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        @{Html.RenderPartial("partials/_Footer"); }

        <!-- Control Sidebar -->
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content("~/Content/plugins/jQuery/jquery-2.2.3.min.js")"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")"></script>
    <!-- DataTables -->
    <script src="@Url.Content("~/Content/plugins/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Content/plugins/datatables/dataTables.bootstrap.min.js")"></script>
    <!-- SlimScroll -->
    <script src="@Url.Content("~/Content/plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/plugins/fastclick/fastclick.js")"></script>
    <script src="@Url.Content("~/Content/plugins/notification/SmartNotification.min.js")"></script>

    <!-- iCheck 1.0.1 -->
    <script src="@Url.Content("~/Content/plugins/iCheck/icheck.min.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/Content/dist/js/app.min.js")"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="@Url.Content("~/Content/dist/js/demo.js")"></script>

    <script src="@Url.Content("~/Scripts/Menu/menus.js")"></script>
<script src="@Url.Content("~/Scripts/Delete_js/delete_js.js")"></script>
    <script src="@Url.Content("~/Content/plugins/select2/select2.full.min.js")"></script>

    <!-- page script -->
    <script>
        $(function () {
            //Initialize Select2 Elements
            $(".select2").select2();
            $("#table1").DataTable();
            //iCheck for checkbox and radio inputs
            $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
                checkboxClass: 'icheckbox_minimal-blue',
                radioClass: 'iradio_minimal-blue'
            });
            //Red color scheme for iCheck
            $('input[type="checkbox"].minimal-red, input[type="radio"].minimal-red').iCheck({
                checkboxClass: 'icheckbox_minimal-red',
                radioClass: 'iradio_minimal-red'
            });
            //Flat red color scheme for iCheck
            $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
                checkboxClass: 'icheckbox_flat-green',
                radioClass: 'iradio_flat-green'
            });

            $('#example2').DataTable({
                "paging": true,
                "lengthChange": true,
                "searching": true,
                "ordering": true,
                "info": true,
                "autoWidth": true,
                "order": [[0, "desc"]],
                "sScrollX": '100%'
            });
        });
    </script>
<script>
    $(document).ready(function () {
        $('body').find('.set-parent-open').parents('li').addClass('active');
//        $('body').find('.edit-btn').hide();

    });
</script>

    
        @RenderSection("Scripts",false);
   

</body>
</html>
